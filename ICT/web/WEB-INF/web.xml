<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>AdminLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteStaffServlet</servlet-name>
        <servlet-class>DeleteStaffServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteStaffServlet</servlet-name>
        <url-pattern>/DeleteStaffServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteNewsServlet</servlet-name>
        <servlet-class>DeleteNewsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteNewsServlet</servlet-name>
        <url-pattern>/DeleteNewsServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddStaffServlet</servlet-name>
        <servlet-class>AddStaffServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddStaffServlet</servlet-name>
        <url-pattern>/AddStaffServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ModifyNameServlet</servlet-name>
        <servlet-class>ModifyNameServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ModifyNameServlet</servlet-name>
        <url-pattern>/ModifyNameServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StaffImageServlet</servlet-name>
        <servlet-class>StaffImageServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StaffImageServlet</servlet-name>
        <url-pattern>/StaffImageServlet</url-pattern>
    </servlet-mapping>



</web-app>